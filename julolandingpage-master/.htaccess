<FilesMatch "\.(?i:gif|jpe?g|png|ico|css|js|swf|webp)$">

 <IfModule mod_headers.c>
   Header set Cache-Control "max-age=31536000, public, must-revalidate"
 </IfModule>

</FilesMatch>

RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.julo.co.id/$1 [R,L]

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ https://www.julo.co.id/ [R]


RewriteEngine On
RewriteCond %{HTTP_HOST} !^www.
RewriteRule ^(.*)$ https://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteEngine On
RewriteCond %{REQUEST_URI} !^rd/$1
RewriteRule ^rd/(.*)$ https://api-dev.julofinance.com/urlshortener/$1 [R=301,L]

RewriteEngine On
RewriteCond %{REQUEST_URI} !^rs/$1
RewriteRule ^rs/(.*)$ https://api-staging.julofinance.com/urlshortener/$1 [R=301,L]

RewriteEngine On
RewriteCond %{REQUEST_URI} !^r/$1
RewriteRule ^r/(.*)$ https://api.julofinance.com/urlshortener/$1 [R=301,L]

